        <script type="text/mustache" id="{{OverlayComponent}}">
<materializer data-uid='{{dataUid}}-mat'
                data-shown='{materializerShown}'
                data-show-done='{dataShowDone}'
                data-hide-done='{dataHideDone}'
                class='ase-overlay-materializer{{#if dataSheet}} ase-overlay-sheetmaterializer ase-overlay-materializersheet{{/if}}'>
    {{#if dataHasScreen}}
        <computed-height data-type="document" data-recompute="{recomputeScreen}"
